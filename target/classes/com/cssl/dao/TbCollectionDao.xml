<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cssl.dao.TbCollectionDao">

    <select id="selectCollection" resultType="map">
      SELECT * FROM tb_collection WHERE uid=#{uid} ORDER BY id DESC
    </select>

</mapper>